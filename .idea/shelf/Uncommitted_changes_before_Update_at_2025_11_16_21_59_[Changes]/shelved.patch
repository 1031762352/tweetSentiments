-// internal/svc/servicecontext.go
 package svc
 
 import (
-	"gorm.io/gorm"
+	"time"
 	"tweetSentiments/internal/config"
+	"tweetSentiments/internal/model"
+
+	"github.com/zeromicro/go-zero/core/logx"
+	"gorm.io/driver/mysql"
+	"gorm.io/gorm"
+	"gorm.io/gorm/logger"
 )
 
+// ServiceContext 服务上下文，存储所有业务依赖（DB、配置、外部客户端等）
 type ServiceContext struct {
-	Config config.Config // 全局配置
-	DB     *gorm.DB      // MySQL连接
-	// 其他依赖（如Twitter客户端、Qdrant客户端等）
+	Config                config.Config                         // 全局配置
+	DB                    *gorm.DB                              // MySQL数据库连接（GORM）
+	InfluencerModel       model.InfluencerModel                 // 大V领域Model
+	TweetModel            model.TweetModel                      // 推文领域Model
+	SentimentResultModel  model.SentimentResultModel            // 情感分析结果Model
+	SentimentKeywordModel model.SentimentKeywordModel           // 关键词领域Model
+	SentimentSummaryModel model.InfluencerSentimentSummaryModel // 情感汇总Model
 }
 
+// NewServiceContext 创建服务上下文（初始化所有依赖）
 func NewServiceContext(c config.Config) *ServiceContext {
-	// 初始化DB、客户端等依赖，使用c中的配置（如c.MySQLConf.DSN）
-	db := initMySQL(c.MySQLConf)
+	// 1. 初始化MySQL连接
+	db, err := initMySQL(c.MySQLConf)
+	if err != nil {
+		logx.Error("MySQL初始化失败：", err)
+		panic(err) // 连接失败直接退出服务
+	}
+
+	// 2. 初始化各领域Model（注入DB连接）
 	return &ServiceContext{
-		Config: c,
-		DB:     db,
+		Config:                c,
+		DB:                    db,
+		InfluencerModel:       model.NewInfluencerModel(db),
+		TweetModel:            model.NewTweetModel(db),
+		SentimentResultModel:  model.NewSentimentResultModel(db),
+		SentimentKeywordModel: model.NewSentimentKeywordModel(db),
+		SentimentSummaryModel: model.NewInfluencerSentimentSummaryModel(db),
+	}
+}
+
+// initMySQL 初始化MySQL连接（GORM v2）
+func initMySQL(mysqlConf config.MySQLConf) (*gorm.DB, error) {
+	// GORM配置
+	gormConfig := &gorm.Config{
+		// 日志配置：开发环境打印SQL，生产环境可调整为warn级别
+		Logger: logger.Default.LogMode(logger.Info),
+		// 禁用默认事务（提高性能，按需开启）
+		SkipDefaultTransaction: true,
+	}
+
+	// 连接MySQL
+	db, err := gorm.Open(mysql.Open(mysqlConf.DSN), gormConfig)
+	if err != nil {
+		return nil, err
 	}
+
+	// 获取底层sql.DB连接，设置连接池参数
+	sqlDB, err := db.DB()
+	if err != nil {
+		return nil, err
+	}
+
+	// 连接池配置（根据业务调整）
+	sqlDB.SetMaxOpenConns(100)                 // 最大打开连接数
+	sqlDB.SetMaxIdleConns(20)                  // 最大空闲连接数
+	sqlDB.SetConnMaxLifetime(30 * time.Minute) // 连接最大生命周期
+	sqlDB.SetConnMaxIdleTime(10 * time.Minute) // 连接最大空闲时间
+
+	logx.Info("MySQL连接初始化成功！")
+	return db, nil
 }
